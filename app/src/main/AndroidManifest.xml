<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.mrsu.medrm">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.LoginFormActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MenuActivity" />
        <activity android:name=".view.ServicesListActivity" />
        <activity android:name=".view.DoctorsListActivity" />
        <activity android:name=".view.OrderPlaceActivity" />
        <activity android:name=".view.HospitalsListActivity" />
        <activity android:name=".view.TimeOrderSelectionActivity" />
        <activity android:name=".view.RegFormActivity" />
        <activity android:name=".view.ResetPasswordActivity" />

    </application>

</manifest>